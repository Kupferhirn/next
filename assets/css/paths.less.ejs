@font-face {
  font-family: 'gkicons';
  src: url(<%= asset_path('fonts/gkicons.eot') + '?-u7jm7z' %>);
  src: url(<%= asset_path('fonts/gkicons.eot') + '?#iefix-u7jm7z' %>) format('embedded-opentype'),
       url(<%= asset_path('fonts/gkicons.woff') + '?-u7jm7z' %>) format('woff'),
       url(<%= asset_path('fonts/gkicons.ttf') + '?-u7jm7z' %>) format('truetype'),
       url(<%= asset_path('fonts/gkicons.svg') + '?-u7jm7z#gkicons' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}

#wrapper,
.masthead {
  background-image: url(<%= asset_path('img/bg.svg') %>);
}