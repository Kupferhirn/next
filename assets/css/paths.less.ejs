@font-face {
  font-family: 'gkicons';
  src: url(<%= asset_path('fonts/gkicons.eot') + '?w2870e' %>);
  src: url(<%= asset_path('fonts/gkicons.eot') + '?#iefixw2870e' %>) format('embedded-opentype'),
       url(<%= asset_path('fonts/gkicons.woff') + '?w2870e' %>) format('woff'),
       url(<%= asset_path('fonts/gkicons.ttf') + '?w2870e' %>) format('truetype'),
       url(<%= asset_path('fonts/gkicons.svg') + '?w2870e#gkicons' %>) format('svg');
  font-weight: normal;
  font-style: normal;
}

#wrapper,
.masthead {
  background-image: url(<%= asset_path('img/bg.svg') %>);
}