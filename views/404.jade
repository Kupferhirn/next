doctype html
html
  head
    title Nicht gefunden.
    link(rel='icon', href='data:image/png;base64,')
    style.
      body {
        margin: 0;
        color: #fff;
        font: 400 12px/20px 'Helvetica Neue', Helvetica, Arial, sans-serif;
        background: #000;
        background-size: cover;
        -webkit-font-smoothing: antialiased;
      }

      body::before {
        content: '';
        position: fixed;
        top: 50%;
        left: 0;
        right: 0;
        opacity: .2;
        background-color: #151515;
        height: 200px;
        margin-top: -117px;
        box-shadow: 0 0 50px 50px #151515;
      }

      a {
        color: #fff;
      }

      input:focus,
      button:focus {
        outline: 0;
      }

      .content {
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 68px;
      }

      .valign-outer {
        display: table;
        width: 100%;
        height: 100%;
      }

      .valign-inner {
        display: table-cell;
        vertical-align: middle;
      }

      .box {
        margin: 32px auto 0;
        width: 400px;
        padding: 20px;
        text-align: center;
      }

      .box h1 {
        margin: 0;
        font-size: 26px;
        line-height: 30px;
        font-weight: bold;
      }

      .box p {
        margin: 0;
        padding: 5px 15px 15px;
        font-size: 14px;
        line-height: 18px;
        font-weight: normal;
      }

      .search-form-row {
        position: relative;
        width: 250px;
        margin: 0 auto;
        border-radius: 14px;
        padding: 2px;
        background-color: rgba(255, 255, 255, .85);
        color: #444;
      }

      .search-form-field {
        width: 100%;
        height: 24px;
      }

      .search-form-field input,
      .search-button {
        -webkit-font-smoothing: antialiased;
        font: 400 12px/20px 'Helvetica Neue', Helvetica, Arial, sans-serif;
      }

      .search-form-field input,
      .bottom-bar {
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
      }

      .search-form-field input {
        margin: 0;
        padding: 2px 15px 2px 30px;
        background: transparent;
        font-size: 13px;
        line-height: 20px;
        font-weight: bold;
        height: 24px;
        width: 100%;
        background: transparent;
        color: #444;
        border: 0;
      }

      .search-button,
      .logo {
        text-indent: -9999px;
        overflow: hidden;
      }

      .search-button {
        position: absolute;
        top: 50%;
        left: 0;
        margin: -15px 0 0 0;
        padding: 0;
        width: 30px;
        height: 30px;
        cursor: pointer;
        background: transparent url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAAIVBMVEUAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABt0UjBAAAACnRSTlMAJwt8psLxUd5DHMjVvQAAAFtJREFUGNNjYBiUgMnEWQCZX7Zq1WIkLmOW5+RVBQg+K1CyqgHB10oCii1E8KUCGBhYVuKWR9fPmOU+eVUjkoXTVq1atUIAxX2iKAqAQlErUP2gugTNTwUDEJAAcwoVy6FG5eYAAAAASUVORK5CYII=') no-repeat;
        border: 0;
        opacity: .37;
      }

      .search-button:hover {
        opacity: .73;
      }

      .bottom-bar {
        position: fixed;
        top: auto;
        right: 0;
        bottom: 0;
        left: 0;
        padding: 17px;
      }

      .logo {
        background: transparent url('data:image/png;base64,') no-repeat;
        width: 173px;
        height: 32px;
        float: left;
        margin: 3px 0 3px 3px;
      }

      .logo a {
        display: block;
        width: 100%;
        height: 100%;
      }

      .bottom-bar .buttons {
        float: right;
        font-size: 0;
        margin-left: 13px;
      }

      .chrome {
        display: inline-block;
        margin: 0 0 0 10px;
        padding: 9px 20px 11px;
        border-radius: 2px;
        text-align: center;
        background-color: rgba(255, 255, 255, .26);
        color: #fff;
        text-decoration: none;
        cursor: pointer;
        border: 0;
        font: bold 16px/18px 'Helvetica Neue', Helvetica, Arial, sans-serif;
        -webkit-font-smoothing: antialiased;
      }

      .chrome:first-child {
        margin: 0;
      }

      .chrome.blue {
        background-color: #529ecc;
      }

      .attribution {
        float: right;
        font-size: 12px;
        text-shadow: 0 1px 1px rgba(0, 0, 0, .8);
        margin: 7px;
      }

      .attribution a {
        display: inline-block;
        vertical-align: middle;
        text-decoration: none;
      }

      .attribution .avatar {
        width: 24px;
        height: 24px;
        margin: 0 0 0 8px;
        border-radius: 2px;
        background-image: url('data:image/png;base64,');
        background-position: 50% 50%;
        background-size: cover;
        box-shadow: 0 1px 1px rgba(0, 0, 0, .3);
      }
  body
    .content
      .valign-outer
        .valign-inner
          .box
            h1 Gähnende Leere
            p Tut mir leid, aber die Seite, nach der du suchst, scheint nicht hier zu sein.
            .search-form-row
              .search-form-field
                input#query(type='text', placeholder='Döner-Suche')
              button.search-button#button(type='button')

    .bottom-bar
      .logo
        a(href='/') Gamekeller
      .buttons
        button.chrome#shuffle(type='button') #HDGDL
        a.chrome.blue(href='https://www.youtube.com/watch?v=GJthaWvfQiY') Robotertränen
      .attribution
        a(href='http://die404seitehabenwirgarnichgeklautdieisnurinspiriertvon.tumblr.com/')
          span Gar nicht geklaut von&nbsp;
          strong Tumblr
        a.avatar(href='http://die404seitehabenwirgarnichgeklautdieisnurinspiriertvon.tumblr.com/')

    script.
      !function(win, doc) {
        var currentBg = localStorage.getItem('gk.404.bg');

        var PARTYTIME = win.PARTYTIME = function() {
          var bgs = [['sherlock', '150%'], ['snape', '150%'], ['bujuhu', ''], ['dwight', '150%'], ['ban', ''], ['endstille', ''], ['dog', ''], ['coke', ''], ['firing', ''], ['putin', 'contain']]
          var pick

          do pick = Math.floor(Math.random() * bgs.length)
          while(bgs[pick][0] === currentBg)

          doc.body.style.backgroundSize = bgs[pick][1]
          doc.body.style.backgroundImage = 'url(/assets/img/404/' + (bgs[pick][0]) + '.gif)'
          currentBg = bgs[pick][0]
          localStorage.setItem('gk.404.bg', currentBg)
        }

        PARTYTIME()

        // Shuffle
        doc.querySelector('#shuffle').addEventListener('click', PARTYTIME, false)

        // Döner Suche
        var field = doc.querySelector('#query')
        var doenerBtn = doc.querySelector('#button')

        doenerBtn.addEventListener('click', function() {
          if(field.value)
            win.location = 'https://www.google.com/search?q=döner+' + encodeURIComponent(field.value)
        }, false)
      }(this, document);