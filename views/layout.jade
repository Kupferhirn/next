doctype html
html
  head
    if title === 'Gamekeller'
      title Gamekeller
    else
      title #{title} | Gamekeller
    != css('css/main.css')
  body

    include partials/nav

    .container
      include partials/flash
      block content

    include partials/footer

    script(src='//ajax.googleapis.com/ajax/libs/jquery/2.1.0/jquery.min.js')
    != js('js/main.js')