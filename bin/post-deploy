#!/usr/bin/env node
'use strict'

var config = require('../lib/config')

if (config.opbeat.active) {
  var opbeat = require('opbeat').start(config.opbeat.credentials)

  opbeat.trackRelease({}, function (err) {
    if (err) {
      console.error(err)
      return process.exit(1)
    }

    process.exit(0)
  })
}